{
	"name"        : "jmf/predefined-strings",
	"type"        : "library",
	"license"     : "MIT",
	"description" : "Wraps and validates preset strings such as e-mail address, IP v4, date, time, MD5 hash, etc.",
	"require"     : {
		"php"          : ">=8.3",
		"ext-mbstring" : "*"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"phing/phing": "^2.17",
		"phpmd/phpmd": "^2.15",
		"phpstan/phpstan": "^1.10",
		"phpunit/phpunit": "^11.1",
		"rector/rector": "^1.0",
		"squizlabs/php_codesniffer": "^3.9"
	},
	"autoload" : {
		"psr-4" : {
			"Jmf\\PredefinedStrings\\" : "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Jmf\\PredefinedStrings\\": "tests/unit/"
        }
    },
    "config": {
        "audit": {
			"abandoned": "report"
		},
        "sort-packages": true
    }
}
